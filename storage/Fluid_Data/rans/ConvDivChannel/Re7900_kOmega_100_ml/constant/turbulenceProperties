FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType RAS;

RAS
{

    RASModel        kOmegaMl;

    turbulence      on;

    printCoeffs     on;
    
    gamma_start     0.0;
    gamma_max       0.8;
    end_ramp	    0.5;
    model_k	    true;
    model_omega     true;

}

